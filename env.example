# Chronicle Configuration

# Databento API Credentials (Required for live market data)
# Sign up at https://databento.com to get these credentials
DATABENTO_API_KEY=your_api_key_here
DATABENTO_USERNAME=your_username_here
DATABENTO_PASSWORD=your_password_here

# Server Configuration (Optional - defaults will be used if not specified)
WS_PORT=8080
DB_PATH=./chronicle_data.db
LOG_DIR=./logs

# Development Settings (Optional)
NODE_ENV=development
DEBUG=false 